<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>

    <table>
        <thead>
            <% header.forEach(column=>{ %>
                <th> <%=column %></th>

                <% }) %>
        </thead>
        <tbody>
            <% body.forEach(rows=>{ %>
                <tr>
                    <% rows.forEach(elements=>{ %>
                        <td><%=elements%></td>
                   <% }) %>
                </tr>

           <% }) %>
        </tbody>
    </table>
</body>

</html>